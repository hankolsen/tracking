{
	"extends": "airbnb-base",
	"env": {
      "browser": true,
      "node": true,
      "es6": true
    },
  "rules": {
    "no-underscore-dangle": 0,
    "no-trailing-spaces": [2, { "skipBlankLines": true }],
    "padded-blocks": 0,
    "func-names": 0,
    "function-paren-newline": 0,
    "no-restricted-globals": 0,
    "wrap-iife": 0,
    "max-len": [2, 120]
  }
}
